{
  "version": "0.2.0",
  "configurations": [
  {
    "name": "Launch Program",
    "program": "${workspaceFolder}/async.js",
    "request": "launch",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/SANJAY_VYAS/JS_INTERNALS",
      "program": "c:/SANJAY_VYAS/JS_INTERNALS/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}